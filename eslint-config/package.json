{
    "name": "@brightlayer-ui/eslint-config",
    "version": "3.0.1",
    "description": "ESLint profile for Brightlayer UI",
    "author": "Brightlayer UI <brightlayer-ui@eaton.com>",
    "main": "index.js",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brightlayer-ui/code-standards.git"
    },
    "bugs": {
        "url": "https://github.com/brightlayer-ui/code-standards/issues"
    },
    "homepage": "https://github.com/brightlayer-ui/code-standards/tree/master/eslint-config#readme",
    "files": [
        "README.md",
        "LICENSE.md",
        "CHANGELOG.md",
        "index.js",
        "package.json",
        "ts.js",
        "tsx.js",
        "blui-rules.js"
    ],
    "scripts": {
        "test": "bash ./verifyArtifacts.sh",
        "tag:package": "npx -p @brightlayer-ui/tag blui-tag",
        "publish:package": "set npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
        "prettier": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html,md}\" --write"
    },
    "peerDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "eslint": ">= 8.6.0",
        "typescript": ">= 4.0.0"
    },
    "dependencies": {
        "@typescript-eslint/parser": "^5.9.0",
        "eslint-config-prettier": ">= 8.0.0"
    },
    "optionalDependencies": {
        "eslint-plugin-react": "^7.28.0"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "brightlayer-ui",
        "quality",
        "standards"
    ],
    "prettier": "@brightlayer-ui/prettier-config",
    "devDependencies": {
        "@brightlayer-ui/prettier-config": "^1.0.3",
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "eslint": ">= 8.6.0",
        "eslint-plugin-react": "^7.28.0",
        "prettier": "2.5.1",
        "typescript": ">= 4.0.0"
    }
}
