{
    "name": "@brightlayer-ui/eslint-config",
    "version": "4.0.1-alpha.0",
    "description": "ESLint profile for Brightlayer UI",
    "author": "Brightlayer UI <brightlayer-ui@eaton.com>",
    "main": "index.js",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/etn-ccis/blui-code-standards.git"
    },
    "bugs": {
        "url": "https://github.com/etn-ccis/blui-code-standards/issues"
    },
    "homepage": "https://github.com/etn-ccis/blui-code-standards/tree/master/eslint-config#readme",
    "files": [
        "README.md",
        "LICENSE.md",
        "CHANGELOG.md",
        "index.js",
        "package.json",
        "ts.js",
        "tsx.js",
        "blui-rules.js"
    ],
    "scripts": {
        "test": "bash ./verifyArtifacts.sh",
        "tag:package": "npx -p @brightlayer-ui/tag blui-tag",
        "publish:package": "set npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
        "prettier": "prettier \"**/**.{ts,tsx,js,jsx,json,css,scss,html,md}\" --write"
    },
    "peerDependencies": {
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@eslint/js": "^9.13.0",
        "typescript-eslint": "^8.11.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-config-prettier":"^9.1.0",
        "@typescript-eslint/parser": "^8.10.0",
        "eslint": ">= 9.12.0"
    },
    "optionalDependencies": {
        "eslint-plugin-react": "^7.37.1"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "brightlayer-ui",
        "quality",
        "standards"
    ],
    "prettier": "@brightlayer-ui/prettier-config",
    "devDependencies": {
        "@brightlayer-ui/prettier-config": "^1.0.3",
        "eslint-plugin-react": "^7.37.1",
        "prettier": "3.4.2",
        "typescript": "5.8.3"
    }
}
